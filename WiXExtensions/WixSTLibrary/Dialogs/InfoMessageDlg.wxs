<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
	<Fragment>
    <UI Id="UIInfoMessageDlg">
      <Dialog Id="InfoMessageDlg" Width="270" Height="105" Title="!(loc.ErrorDlg_Title)">
        <Control Id="InfoIcon" Type="Icon" X="15" Y="15" Width="24" Height="24" IconSize="32" FixedSize="yes" Text="WixUI_Ico_Info" />
        <Control Id="InfoText" Type="Text" X="48" Y="15" Width="205" Height="60" TabSkip="no" NoPrefix="yes" Text="[STRINGMESSAGE]" />
        <Control Id="Ok" Type="PushButton" X="107" Y="80" Width="56" Height="17" TabSkip="no" Text="!(loc.WixUIOK)">
          <Publish Event="EndDialog" Value="Return" />
        </Control>
      </Dialog>
    </UI>
	</Fragment>
</Wix>